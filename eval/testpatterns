SELECT test_train.id, test_train2.id, age, books, test_train2.name FROM test_train, test_train2 WHERE test_train.id = test_train2.id;
SELECT test_train.name, age, salary, test_train2.name, books FROM test_train, test_train2 WHERE test_train.age=test_train2.books;
